    curl -s "$url" | jq '.'
    pause
}
github_repo_stalk() {
    show_header
    echo "=== Stalk Repo GitHub ==="
    read -p "Masukkan username pemilik repo (contoh: TheyanzXD): " user
    if [ -z "$user" ]; then
        user="TheyanzXD"
        echo "Input kosong, menggunakan contoh: $user"
    fi
    read -p "Masukkan nama repo (contoh: Vercel): " repo
    if [ -z "$repo" ]; then
        repo="Vercel"
        echo "Input kosong, menggunakan contoh: $repo"
    fi
    encoded_user=$(urlencode "$user")
    encoded_repo=$(urlencode "$repo")

    url="https://zelapioffciall.koyeb.app/stalk/githubrepo?user=${encoded_user}&repo=${encoded_repo}"
    echo "Mengambil data dari: $url"
    curl -s "$url" | jq '.'
    pause
}
