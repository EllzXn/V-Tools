    read -p "Masukkan URL (contoh: https://github.com): " url_input
    if [ -z "$url_input" ]; then
        url_input="https://github.com"
        echo "Input kosong, menggunakan contoh: $url_input"
    fi
    encoded_url=$(urlencode "$url_input")

    api_url="https://api.naze.biz.id/tools/get-headers?url=${encoded_url}&apikey=${NAZE_API_KEY}"
    response=$(curl -s "$api_url")
    echo "$response" | jq '.'
    pause
}
