cek_gender() {
    show_header
    echo "=== Cek Gender Nama ==="
    read -p "Masukkan nama (contoh: yan): " name
    if [ -z "$name" ]; then
        name="yan"
        echo "Input kosong, menggunakan contoh: $name"
    fi
    encoded_name=$(urlencode "$name")

    url="https://api.naze.biz.id/primbon/cekgender?name=${encoded_name}&apikey=${NAZE_API_KEY}"
    echo "Mengecek gender nama: $name"
    echo "Mengakses: $url"
    response=$(curl -s "$url")
    echo "$response" | jq '.'
    pause
}
